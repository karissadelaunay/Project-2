<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Category</th>

        <th></th>
      </tr>
    </thead>
    <tbody>
      <% categories.forEach(function(c) { %>
        <tr>
          <td><%= c.category %></td>
          <td><a href="/categories<%= c._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>